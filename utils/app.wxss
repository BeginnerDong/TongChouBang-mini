/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/***********navbar***********/
.navbar{
	height:110rpx;
	width:100%;
	position:fixed;
	bottom:0rpx;
	z-index:999;
	background:#fff;
	display:flex;
	justify-content:center;
	align-items:center;
	box-shadow:0rpx 0rpx 15rpx #D5D5D5;
}
.nav_img image{
	width:46rpx;
	height:46rpx;
}
.navbar_item{
	width: 25%;
	text-align:center;
}
.navbar .text{
	color:#999;
	font-size:24rpx;
	margin-top: 8rpx;
}
.navbar .this-text{
	color:#FE546F;
}
.nav_img{
	width:48rpx;
	height:48rpx;
	margin:0 auto;
}
/***********navbar***********/
